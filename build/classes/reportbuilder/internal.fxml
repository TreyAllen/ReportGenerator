      <Label fx:id="reportGenLabel" alignment="CENTER" layoutX="20.0" layoutY="86.0" prefHeight="68.0" prefWidth="381.0" stylesheets="@styles/styles.css" text="Report Generator">
         <font>
            <Font name="System Bold Italic" size="46.0" />
         </font>
      </Label>
      
      <Label fx:id="versionLabel" layoutX="402.0" layoutY="122.0" prefHeight="17.0" prefWidth="37.0" stylesheets="@styles/styles.css" text="v#">
