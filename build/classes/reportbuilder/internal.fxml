      </Label>
      <Button fx:id="logoutButton" layoutX="340.0" layoutY="21.0" mnemonicParsing="false" onAction="#logoutButtonHandleAction" prefHeight="5.0" prefWidth="57.0" styleClass="darkButton" stylesheets="@styles/styles.css" text="Logout" textFill="WHITE">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
      <Button fx:id="getReportButton" layoutX="127.0" layoutY="192.0" mnemonicParsing="false" onAction="#getResultsButtonHandleAction" prefHeight="25.0" prefWidth="191.0" styleClass="maroonButton" stylesheets="@styles/styles.css" text="Get Report" textFill="WHITE">
