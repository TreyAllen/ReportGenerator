            <Font name="System Bold" size="12.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
      <Label fx:id="reportStatusLabel" alignment="TOP_CENTER" contentDisplay="CENTER" layoutY="229.0" prefHeight="17.0" prefWidth="443.0" styleClass="blueText" stylesheets="@styles/styles.css" text="Generating report..." visible="false" />
