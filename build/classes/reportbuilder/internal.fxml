            <Font name="System Italic" size="12.0" />
         </font>
      </Label>
      <Label fx:id="amsLabel" layoutX="34.0" layoutY="80.0" prefHeight="17.0" prefWidth="165.0" stylesheets="@styles/styles.css" text="Alarm Monitoring Services Inc." textFill="#9e021d">
