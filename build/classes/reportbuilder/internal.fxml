<AnchorPane id="AnchorPane" prefHeight="357.0" prefWidth="443.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="reportbuilder.InternalController">
    
    <children>
      <Pane fx:id="bkgrndPane" prefHeight="357.0" prefWidth="443.0" stylesheets="@styles/styles.css" />
      <HBox fx:id="progressSpinner" layoutX="200.0" layoutY="251.0" prefHeight="44.0" prefWidth="45.0" styleClass="loadingGIFHbox" stylesheets="@styles/styles.css" visible="false" />
